  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["www/**/*", ".eslintrc.cjs", "esbuild.config.js"],
  "exclude": ["**/dist/*.d.ts"]
}
