{
  "name": "@harleys-website/me-darkmode-toggle",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "yarn dev:server & yarn dev:scss",
    "dev:server": "yarn wds element=me-darkmode-toggle",
    "dev:scss": "sass -w ../../../css",
    "wds": "wds --config ../wds.config.mjs",
    "build": "yarn build:esbuild && yarn build:tsc",
    "build:esbuild": "node ../esbuild.config.mjs component=me-darkmode-toggle",
    "build:tsc": "tsc -p tsconfig.build.json"
  },
  "author": "Harley Lang",
  "license": "MIT",
  "dependencies": {
    "esbuild": "^0.14.8",
    "esbuild-minify-templates": "^0.7.2",
    "esbuild-plugin-svg": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@web/dev-server": "^0.1.29",
    "@web/dev-server-esbuild": "^0.2.16",
    "sass": "^1.53.0",
    "typescript": "^4.5.4"
  }
}
