{
  "name": "@harleys-website/me-carousel",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n 'SERVER,SCSS' -c 'bgBlue.bold,bgMagenta.bold' 'yarn dev:server' 'yarn dev:scss'",
    "dev:server": "yarn wds element=me-carousel",
    "dev:scss": "sass -w ../../css",
    "wds": "wds --config ../wds.config.mjs",
    "build": "node ./esbuild.config.js && tsc -p tsconfig.build.json"
  },
  "author": "Harley Lang",
  "license": "MIT",
  "dependencies": {
    "esbuild": "^0.14.8",
    "esbuild-minify-templates": "^0.7.2",
    "esbuild-plugin-svg": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.4",
    "@web/dev-server": "^0.1.29",
    "@web/dev-server-esbuild": "^0.2.16",
    "concurrently": "^7.3.0",
    "sass": "^1.53.0",
    "typescript": "^4.5.4"
  }
}
